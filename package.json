{
  "name": "codeadrian",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "clean": "rm -rf _site",
    "hasher": "node csshash.js",
    "start": "npm-run-all clean hasher --parallel dev:*",
    "dev:postcss": "postcss src/assets/css/index.css -o _site/css/index.css --config ./postcss.config.js -w",
    "dev:eleventy": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --quiet --incremental",
    "build": "NODE_ENV=production npm-run-all clean hasher --parallel prod:*",
    "prod:postcss": "postcss src/assets/css/index.css -o _site/css/$(cat csshash) --config ./postcss.config.js",
    "prod:eleventy": "ELEVENTY_ENV=production npx @11ty/eleventy --output=./_site"
  },
  "keywords": [],
  "author": "Adrian Bece <adrianbece@live.com>",
  "license": "MIT",
  "browserslist": [
    "last 3 versions",
    "not dead",
    "not ie > 1",
    "not op_mini all",
    "not op_mob > 1"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^1.0.0",
    "cssnano": "^5.0.12",
    "eleventy-plugin-error-overlay": "^0.1.2",
    "eleventy-plugin-svg-contents": "^0.7.0",
    "glob": "^7.2.0",
    "glob-all": "^3.2.1",
    "html-minifier": "^4.0.0",
    "markdown-it": "^12.3.0",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.0.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@glidejs/glide": "^3.5.2",
    "eleventy-plugin-babel": "^1.0.1",
    "eleventy-plugin-purgecss": "^0.2.0",
    "postcss-nested": "^5.0.6"
  }
}
