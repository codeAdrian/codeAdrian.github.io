@font-face {
    font-family: 'devstar';
    src: url('/assets/fonts/devstar.ttf?ipmzvv') format('truetype'),
        url('/assets/fonts/devstar.woff?ipmzvv') format('woff'),
        url('/assets/fonts/devstar.svg?ipmzvv#devstar') format('svg');
    font-weight: normal;
    font-style: normal;
}

.icon {
    font-family: 'devstar' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

