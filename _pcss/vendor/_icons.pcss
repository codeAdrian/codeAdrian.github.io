@font-face {
    font-family: "devstar";
    src: url("/assets/fonts/devstar.ttf?ytkptp") format("truetype"),
        url("/assets/fonts/devstar.woff?ytkptp") format("woff"),
        url("/assets/fonts/devstar.svg?ytkptp#devstar") format("svg");
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

.icon {
    font-family: "devstar" !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

